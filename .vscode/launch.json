{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "progress_foundry",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file",
                "development.env"
            ],
            "toolArgs": [
                "-d",
                "chrome",
                "--web-browser-flag",
                "--disable-web-security"
            ]
        },
        {
            "name": "progress_foundry (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define-from-file",
                "development.env"
            ]
        },
        {
            "name": "progress_foundry (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define-from-file",
                "development.env"
            ]
        }
    ]
}